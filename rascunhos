numeros = []
for i in range(3):
    numeros.append(str(i))

letras = []
for i in range (3):
    letras.append(chr(97 + i))

letras_upper = []
for i in range (3):
    letras_upper.append(chr(65 + i))

for numero in numeros:
    for letra in letras:
        for letra_upper in letras_upper:
            senha = numero + letra + letra_upper